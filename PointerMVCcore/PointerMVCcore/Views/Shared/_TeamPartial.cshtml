@model TeamSocialModel

<section class="site-section border-bottom" id="team-section">
    <div class="container">
        <div class="row mb-5">
            <div class="col-md-5 text-left">
                <h2 class="section-title mb-3">Our Dedicated <br> Professionals</h2>
                <p class="lead">Lorem ipsum dolor sit amet consectetur adipisicing elit. Minus minima neque tempora reiciendis.</p>
            </div>
        </div>
        <div class="row">

            @foreach (var team in Model.teamModel)
            {

                <div class="col-md-6 col-lg-3 mb-4">
                    <div class="team-member">
                        <figure>
                            <ul class="social">
                                @foreach (var item in Model.socialmodel)
                                {
                                    
                                    <li><a href="@item.Href"><span title="@item.Name" class="@item.CssName"></span></a></li>
                                }
                               
                            </ul>
                            <img src="images/@team.Image" alt="Image" class="img-fluid">
                        </figure>
                        <div class="p-3">
                            <h3>@team.Name</h3>
                            <span class="position">@team.Position</span>
                        </div>
                    </div>
                </div>

            }



        </div>
    </div>
</section>

